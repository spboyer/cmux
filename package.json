{
  "name": "cmux",
  "productName": "cmux",
  "version": "0.16.0",
  "description": "Workspace and agent manager for multiple repositories",
  "main": ".webpack/main",
  "private": true,
  "scripts": {
    "start": "electron-forge start",
    "package": "node scripts/prepare-node-runtime.js && electron-forge package",
    "make": "node scripts/prepare-node-runtime.js && electron-forge make",
    "publish": "node scripts/prepare-node-runtime.js && electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "npm run package && playwright test"
  },
  "keywords": [],
  "author": {
    "name": "Ian Philpot",
    "email": "ianphil@microsoft.com"
  },
  "license": "MIT",
  "build": {
    "publish": {
      "provider": "github",
      "owner": "ipdelete",
      "repo": "cmux"
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-deb": "^7.11.1",
    "@electron-forge/maker-rpm": "^7.11.1",
    "@electron-forge/maker-squirrel": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.11.1",
    "@electron-forge/plugin-fuses": "^7.11.1",
    "@electron-forge/plugin-webpack": "^7.11.1",
    "@electron/fuses": "^1.8.0",
    "@github/copilot-sdk": "^0.1.23",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.5",
    "@testing-library/react": "^16.3.0",
    "@timfish/forge-externals-plugin": "^0.2.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.7",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "css-loader": "^6.11.0",
    "electron": "40.1.0",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "node-loader": "^2.1.0",
    "playwright": "^1.52.0",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.4",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@homebridge/node-pty-prebuilt-multiarch": "^0.13.1",
    "@vscode/codicons": "^0.0.44",
    "@xterm/addon-clipboard": "^0.2.0",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.7.3",
    "monaco-editor": "^0.55.1",
    "monaco-editor-webpack-plugin": "^7.1.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  }
}
